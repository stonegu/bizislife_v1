<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">    
    
<hibernate-mapping>
    <class name="com.bizislife.core.hibernate.pojo.OrgVisibleGroup" table="accountgroup_organization">
		<cache usage="read-write" />

        <composite-id>
            <key-property name="accountgroup_id" type="long" column="accountgroup_id" />
            <key-property name="visibleOrgs_id" type="long" column="visibleOrgs_id"/>
        </composite-id>

    </class>
</hibernate-mapping>